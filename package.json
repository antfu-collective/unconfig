{
  "name": "unconfig",
  "version": "0.3.5",
  "packageManager": "pnpm@7.9.0",
  "description": "A universal solution for loading configurations.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/unconfig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/unconfig.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/unconfig/issues"
  },
  "keywords": [
    "config"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./presets": {
      "types": "./dist/presets.d.ts",
      "require": "./dist/presets.cjs",
      "import": "./dist/presets.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest"
  },
  "dependencies": {
    "@antfu/utils": "^0.5.2",
    "defu": "^6.1.0",
    "jiti": "^1.14.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.2",
    "@antfu/ni": "^0.17.2",
    "@types/node": "^18.7.13",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.0",
    "esno": "^0.16.3",
    "typescript": "^4.8.2",
    "unbuild": "^0.8.9",
    "vitest": "^0.22.1"
  }
}
